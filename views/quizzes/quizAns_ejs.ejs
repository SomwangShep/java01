<% include ../partials/header.ejs %>  
  <div class="jumbotron">   
    <div style="text-align: center;">
      <h2>Score: <%= (score/cnt)*100 %>%</h2>
      <p>
        <a href="/quizzes/quiz1" class="btn btn-warning" role="button">Try again</a>
        <a href="/quizzes" class="btn btn-warning" role="button">Exit Quiz</a>
      </p>
    </div>  
    
    <!--- #1 --->
    <div class="row">
      <div class="col">
        1. <%= hea %>
        <code>
        <p><%- qs %></p>
        <p><%= prt %></p></code>  
      </div>
      <div class="col">
        <div style="font-family:courier;">
          <p>'<%= usrAns %>' :You Answerd</p>
          <p>'<%= javAns %>' :Correct Answer</p>
        </div>
        
        <!--apply color-->
        <% if(chk === "Incorrect") { %>
          <strong style="color:red;">Incorrect</strong>
        <% } else { %>
          <strong style="color:blue;">Correct</strong>
        <% } %>
         <!--End apply color-->
      </div>
    </div>

